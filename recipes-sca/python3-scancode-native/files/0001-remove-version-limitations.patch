From 9cc2df95f185555bdd94b855146afa34fd328d2a Mon Sep 17 00:00:00 2001
From: Konrad Weihmann <kweihmann@outlook.com>
Date: Sun, 29 Nov 2020 21:59:38 +0100
Subject: [PATCH] remove version limitations

on
- pygments
- typing
- ftfy
- bitarray
- fingerprints
- chardet

Signed-off-by: Konrad Weihmann <kweihmann@outlook.com>
---
 setup.py                                             | 12 ++++++------
 .../requests-2.24.0/requests/__init__.py             |  2 +-
 .../pypi/setup.py-versions/requests-2.24.0/setup.py  |  2 +-
 3 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/setup.py b/setup.py
index d7f026417..990b2ef87 100644
--- a/setup.py
+++ b/setup.py
@@ -140,7 +140,7 @@ setup(
         'py2_ipaddress >= 2.0, <3.5; python_version < "3"',
         'urlpy',
         'publicsuffix2',
-        'fingerprints >= 0.6.0, < 1.0.0',
+        'fingerprints >= 0.6.0',
 
         # commoncode
         'commoncode >= 20.09',
@@ -153,7 +153,7 @@ setup(
         'plugincode',
 
         # licensedcode
-        'bitarray >= 0.8.1, < 1.0.0',
+        'bitarray >= 0.8.1',
         'intbitset >= 2.3.0,  < 3.0',
         'boolean.py >= 3.5,  < 4.0',
         'license_expression >= 0.99',
@@ -168,7 +168,7 @@ setup(
         'six',
         'pdfminer.six >= 20170720',
         'pycryptodome >= 3.4',
-        'chardet >= 3.0.0, <4.0.0',
+        'chardet >= 3.0.0',
 
         # typecode
         'typecode',
@@ -185,11 +185,11 @@ setup(
         'gemfileparser >= 0.7.0',
         'pkginfo >= 1.5.0.1',
         'dparse2',
-        'pygments >= 2.4.2, <2.5.1',
+        'pygments >= 2.4.2',
 
         # used to fix mojibake in Windows PE
         # for now we use the evrsion that works on both Python 2 and 3
-        'ftfy <  5.0.0',
+        'ftfy',
 
         # scancode
         # Click 7.0 is broken https://github.com/pallets/click/issues/1125
@@ -197,7 +197,7 @@ setup(
         'colorama >= 0.3.9',
         'pluggy >= 0.4.0, < 1.0',
         'attrs >= 18.1, !=20.1.0',
-        'typing >=3.6, < 3.7',
+        'typing >=3.6',
 
         # scancode outputs
         'jinja2 >= 2.7.0, < 3.0.0',
diff --git a/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/requests/__init__.py b/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/requests/__init__.py
index db090aa70..251e776a0 100644
--- a/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/requests/__init__.py
+++ b/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/requests/__init__.py
@@ -66,7 +66,7 @@ def check_compatibility(urllib3_version, chardet_version):
     major, minor, patch = chardet_version.split('.')[:3]
     major, minor, patch = int(major), int(minor), int(patch)
     # chardet >= 3.0.2, < 3.1.0
-    assert major == 3
+    assert major >= 3
     assert minor < 1
     assert patch >= 2
 
diff --git a/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/setup.py b/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/setup.py
index 2da9ba07c..6bd6e5796 100644
--- a/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/setup.py
+++ b/tests/packagedcode/data/pypi/setup.py-versions/requests-2.24.0/setup.py
@@ -42,7 +42,7 @@ if sys.argv[-1] == 'publish':
 packages = ['requests']
 
 requires = [
-    'chardet>=3.0.2,<4',
+    'chardet>=3.0.2',
     'idna>=2.5,<3',
     'urllib3>=1.21.1,<1.26,!=1.25.0,!=1.25.1',
     'certifi>=2017.4.17'
-- 
2.25.1

